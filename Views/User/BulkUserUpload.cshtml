<style>
    
</style>
@using (Html.BeginForm("BulkUserUpload", "User", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="panel">
        <div class="panel-body">
            <div class="page-heading">
                <h2 class="pull-left">Create User</h2>
            </div>
            <div class="form-horizontal">
                <div class="form-group">
                    <label class="col-sm-3 control-label">Select File To Upload <span class="form-colon">:</span></label>
                    <div class="col-sm-6">
                        <input type="file" name="file" class="custom-file-input" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label"></label>
                    <div class="col-sm-6">
                        <input type="submit" value="Upload" class="btn btn-primary" />
                    </div>
                </div>
            </div>
        </div>
    </div>
            }
